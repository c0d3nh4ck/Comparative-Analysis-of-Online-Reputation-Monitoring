<script type="text/javascript">
<!--//

$(document).ready(function($e) {
	refreshTwitPics();
	$(".twitpic-refresh").click(function(e) {
		e.preventDefault();
		refreshTwitPics();
	});
});

function refreshTwitPics() {

	$("#twitpics-holder").height($("#twitpics").height()+24);
	$("#twitpics").animate( {'opacity':0}, 250, function() {

		$.getJSON(
			'http://api.twitpic.com/2/users/show.jsonp?callback=?',
			{username: 'thescript'},
			function(twitpic) {
				$('#twitpics').html('');
				var i=0;
				$.each(twitpic.images, function(i,item){
					if (i<12)
						$('#twitpics').append('<a title="'+item.message+'" href="http://twitpic.com/'+item.short_id+'"><img src="http://twitpic.com/show/thumb/'+item.short_id+'" width="80" height=80" alt="'+item.message+'" /></a>');
					i++;
			});
			$('#twitpics').append('<div class="clear"><!-- --></div>');
			var newheight = $("#twitpics").outerHeight();
			$("#twitpics-holder").animate(
				{height: newheight+16},
				{duration: 250}
			);

			$("#twitpics").fadeIn('slow');
			$("#twitpics").animate( {'opacity':1}, 250);

		});

	});

}

//-->
</script>	
