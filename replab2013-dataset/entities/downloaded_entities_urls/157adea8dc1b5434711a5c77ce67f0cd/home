function linkclick(num1, num2)
{
	if (typeof(playerWindow) == 'undefined') {
		if (num2) {
			playerWindow = window.open("/es/player#?album="+num1+"&track="+num2, 'player', 'menubar=1,resizable=1,width=1000,height=600');
